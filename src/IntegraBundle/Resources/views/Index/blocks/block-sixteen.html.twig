<section class="block16">
    <div class="wrapper">
        <div class="photo-container">
            {% image '@IntegraBundle/Resources/public/assets/Index/images/photos/21-1.jpg' %}
                <img src="{{ asset_url }}" alt="" />
            {% endimage %}
        </div>
        <div class="about">
